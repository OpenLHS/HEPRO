    <owl:Class rdf:about="http:/purl.obolibrary.org/obo/HEPRO_0000001">
        <rdfs:subClassOf rdf:resource="http:/purl.obolibrary.org/obo/NOYO_0000001"/>
        <obo:IAO_0000115 xml:lang="en">A request specifying the execution of some health procedures.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">health procedure request</rdfs:label>
    </owl:Class>
    


    <!-- http:/purl.obolibrary.org/obo/HEPRO_0000002 -->

    <owl:Class rdf:about="http:/purl.obolibrary.org/obo/HEPRO_0000002">
        <rdfs:subClassOf rdf:resource="http:/purl.obolibrary.org/obo/HEPRO_0000001"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/IAO_0000136"/>
